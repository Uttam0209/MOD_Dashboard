
@{
    ViewBag.Title = "excelupload";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>

    <h2 class="jumbotron p-3 mt-2">Excel Import</h2>
    <form method="post" enctype="multipart/form-data">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-6">
                    <input name="file" class="form-control" type="file" required style="padding:4px;" />
                </div>
                <div class="col-md-6">
                    <div class="d-flex">
                        <button type="submit" class="btn btn-info">Import</button>
                        <h4 class="text-danger ml-2">@TempData["DataUpload"]</h4>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>

