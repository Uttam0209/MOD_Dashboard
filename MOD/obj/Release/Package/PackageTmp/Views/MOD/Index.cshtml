    function editData(type, selfObj, NodalCompID) {

        if (type == 1) {
            $.ajax({
                type: "POST",
                url: "/MODDelete?ID=" + NodalCompID,
                success: function (result) {
                       $(selfObj).closest('tr').remove();
                }
            })
        } else {
            $.ajax({
                type: "POST",
                url: "/MOD/Edit?ID=" + NodalCompID,
                success: function (result) {
                    alert();
                    $('#exampleEditModal').modal('show');
                }
            })
        }

    }
