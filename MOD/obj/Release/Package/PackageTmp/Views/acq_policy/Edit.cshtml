                <div class="row">

                    <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                        <div class="col-md-4">
                            <label for="date" style="float: left;    font-size: 22px;    font-weight: 700;">From Date: <span class="danger">*</span></label>
                        </div>
                        <div class="col-md-8">
                            <input type="date" id="Fdate" required class="form-control" value="@Model.fdate.Value.ToString("yyyy-MM-dd")" max="@DateTime.Now.Year-@DateTime.Now.Month-@DateTime.Now.Day" name="fdate" />
                            @Html.ValidationMessageFor(x => x.fdate, null, new { @class = "danger" })
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                        <div class="col-md-4">
                            <label for="date" style="float: left;    font-size: 22px;    font-weight: 700;">To Date: <span class="danger">*</span></label>
                        </div>
